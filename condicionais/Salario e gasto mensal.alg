algoritmo "salario"
// Função :
// Autor :
// Data : 18/05/2023
// Seção de Declarações 
var

salario, gastomensal:real

inicio
// Seção de Comandos

escreval("salario mensal")
leia(salario)

escreval("gasto mensal")
leia(gastomensal)

se (salario < gastomensal) entao
   escreval ("saldo negativo")
   
senao
     escreval("saldo positivo")

fimse
 
fimalgoritmo
